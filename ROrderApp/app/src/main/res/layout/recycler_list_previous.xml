<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="5dp"
    android:orientation="vertical">
    <!--android:focusableInTouchMode="true"
    android:overScrollMode="always"
    android:focusable="true"-->

    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/RecyclerCard_Previous"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp">

        <!-- <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:orientation="vertical">-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
                 android:id="@+id/RecyclerCardHeader"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:layout_marginBottom="4dp"
                 >-->

            <LinearLayout
                android:id="@+id/pre_headerChef"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/recycle_chef_order_header"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="4dp"
                android:translationZ="5dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:layout_weight="0.5"
                    android:orientation="vertical"
                    android:translationZ="4dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <com.vibeosys.rorderapp.views.CustomTextView
                            android:id="@+id/recyclerTxtTableNoTitle_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="Table "
                            android:textColor="@color/black_color"
                            android:textSize="12sp" />
                        <com.vibeosys.rorderapp.views.CustomTextView
                            android:id="@+id/recyclerTxtTakeAwayTitle_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="TakeAway"
                            android:textColor="@color/black_color"
                            android:textSize="12sp" />

                        <com.vibeosys.rorderapp.views.CustomTextView
                            android:id="@+id/recyclerTableNoTab_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:text="1"
                            android:textColor="@color/black_color"
                            android:textSize="16sp" />

                        <com.vibeosys.rorderapp.views.CustomTextView
                            android:id="@+id/txtHash_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="Order "
                            android:textColor="@color/black_color"
                            android:textSize="12sp" />

                        <com.vibeosys.rorderapp.views.CustomTextView
                            android:id="@+id/recyclerOrderNoTab_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:text=""
                            android:textColor="@color/black_color"
                            android:textSize="16sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/hrsGlassIcon_Previous"
                            android:layout_width="10dp"
                            android:layout_height="12dp"
                            android:layout_gravity="bottom"
                            android:layout_marginBottom="2dp"
                            android:layout_weight="1"
                            android:src="@drawable/icon_hours_glass" />

                        <com.vibeosys.rorderapp.views.CustomTextView
                            android:id="@+id/recyclerOrderTime_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:layout_weight="2"
                            android:focusable="false"
                            android:text="29 mins ago"
                            android:textColor="@color/black_color"
                            android:textSize="12sp" />
                    </LinearLayout>

                </LinearLayout>


                <!--   <TextView
                       android:id="@+id/txtServedName"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:gravity="end"
                       android:layout_weight="1"
                       android:text="Served by"
                       android:textSize="10sp"
                       android:textColor="@android:color/darker_gray"/>-->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:layout_weight="0.5"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/servedByIcon_Previous"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_weight="1"
                        android:src="@drawable/ico_served_by_black" />
                    <!-- <ImageView
                         android:id="@+id/demo"
                         android:layout_width="wrap_content"
                         android:layout_height="10dp"
                         android:layout_weight="1"
                         android:padding="4dp"
                         android:src="" />-->


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:orientation="vertical">

                    <com.vibeosys.rorderapp.views.CustomTextView
                        android:id="@+id/recyclerServedByName_Previous"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="DemoName"
                        android:textColor="@color/black_color"
                        android:textSize="16sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="1.5dp"
                        android:layout_weight="1"
                        android:gravity="bottom"
                        android:orientation="horizontal">

                        <!--<TextView
                            android:id="@+id/recyclerOrderDoneBtn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="COMPLETE"
                            android:textColor="@color/White"
                            android:textSize="14sp" />-->

                        <!--<Button
                            android:id="@+id/recyclerOrderDoneBtn_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:background="@drawable/button_custom"
                            android:focusable="false"
                            android:text="Complete"
                            android:textColor="@color/White"
                            android:textSize="12sp" />-->

                        <ImageView
                            android:id="@+id/recyclerChefOrderDonIcon_Previous"
                            android:layout_width="wrap_content"
                            android:layout_height="35dp"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_action_tick_chef" />
                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>
            <!--</android.support.v7.widget.CardView>-->

            <ListView
                android:id="@+id/recyclerMenuList_Previous"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="4dp"
                android:paddingBottom="2dp"
                android:scrollbarAlwaysDrawVerticalTrack="false"
                android:scrollbars="none">

            </ListView>
        </LinearLayout>


        <!--</LinearLayout>-->


    </android.support.v7.widget.CardView>
</LinearLayout>
