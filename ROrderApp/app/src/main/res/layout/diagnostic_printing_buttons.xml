<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/show_search_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Show Search Result"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/print_kot_diagnostic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"

            android:clickable="false"
            android:text="@string/print_kot_btn_diagnostic"/>
        <Button
            android:id="@+id/print_bill_diagnostic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:layout_weight="1"

            android:text="@string/print_bill_btn_diagnostic"/>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/Printer_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    </LinearLayout>

</LinearLayout>